{
  "schemaVersion": 1,
  "id": "liondisplays",
  "version": "${version}",
  "name": "LionDisplays",
  "description": "Some additional displays that can be used by the Server",
  "authors": [
    "LionK08"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/liondisplays/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "dev.lionk.liondisplays.client.LiondisplaysDataGenerator"
    ],
    "client": [
      "dev.lionk.liondisplays.client.LiondisplaysClient"
    ],
    "main": [
      "dev.lionk.liondisplays.Liondisplays"
    ],
    "modmenu": [
      "dev.lionk.liondisplays.client.configuration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "liondisplays.mixins.json",
    {
      "config": "liondisplays.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "suggests": {
    "cloth-config2": ">=8.0.0"
  }
}
